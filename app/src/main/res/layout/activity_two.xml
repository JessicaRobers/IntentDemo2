<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/twoLayout"
    android:layout_height="match_parent"
    tools:context=".TwoActivity">


    <ImageView
        android:id="@+id/supermoonID"
        android:layout_width="171dp"
        android:layout_height="331dp"
        android:layout_marginEnd="224dp"
        android:contentDescription="@string/contentD_supermoon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/supermoon"
        tools:srcCompat="@drawable/supermoon" />

    <ImageView
        android:id="@+id/waterfallID"
        android:layout_width="171dp"
        android:layout_height="326dp"
        android:layout_marginEnd="16dp"
        android:contentDescription="@string/contentD_waterfall"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.513"
        app:srcCompat="@drawable/waterfall"
        tools:srcCompat="@drawable/waterfall" />
</android.support.constraint.ConstraintLayout>